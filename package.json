{
  "name": "es6",
  "version": "1.0.0",
  "description": "",
  "bin": "./distr/index.js",
  "pkg": {
    "assets": [
      "./distr/html/**/*"
    ]
  },
  "scripts": {
    "build": "babel src -d nodeDist --presets es2015",
    "serve": "babel src -d nodeDist --presets es2015 && cd distr && node index.js",
