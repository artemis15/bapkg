    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "dependencies": {
    "carlo": "^0.9.43",
    "glob-promise": "^3.4.0",
    "globby": "^9.0.0"
  }
}
